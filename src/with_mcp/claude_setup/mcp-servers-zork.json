{
  "mcpServers": {
    "zork-game": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "pyFrotz==0.1.5",
        "src/frotz/local_game_mcp_server.py",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "src"
      }
    },
    "zork-game-with-logging": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "pyFrotz==0.1.5",
        "src/with_mcp/local_main_mcp_server.py",
        "--debug"
      ],
      "env": {
        "PYTHONPATH": "src"
      }
    }
  }
}